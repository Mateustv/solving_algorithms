   "source": [
    "def isPalindrome(s):\n",
    "    a = \"\"\n",
    "    for i in s:\n",
    "        if i.isalpha():\n",
    "            a += i.lower()\n",
    "        if i.isnumeric():\n",
    "            a += i\n",
    "    return a == a[::-1]"
   ]
  },
